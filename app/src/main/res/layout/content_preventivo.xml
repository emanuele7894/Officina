<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             android:background="@color/colorSfondo">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@android:drawable/editbox_dropdown_light_frame"
            android:id="@+id/imageView11"
            android:scaleType="fitXY" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="25dp" android:layout_marginStart="10dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp" android:layout_marginBottom="25dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="NOME E COGNOME"
            android:layout_width="0dp"
            android:layout_height="26dp"
            android:id="@+id/textView31" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView11" app:layout_constraintStart_toStartOf="@+id/imageView11"
            android:layout_marginStart="8dp"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:background="@drawable/border_text1"
            android:textAlignment="center"
            android:fontFamily="@font/heebo_bold" android:typeface="normal" android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/textView32" android:gravity="center_horizontal|center_vertical"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:layout_width="1dp"
            android:layout_height="0dp"
            android:id="@+id/textView32" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/imageView11"
            app:layout_constraintEnd_toEndOf="@+id/imageView11"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="@+id/imageView11"
            android:visibility="invisible"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="26dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView11"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView11" android:background="@drawable/border_text2"
            android:textAlignment="center" app:layout_constraintStart_toEndOf="@+id/textView31"
            android:fontFamily="@font/heebo" android:textSize="12sp" android:textColor="@color/colorPrimary"
            android:gravity="center_horizontal|center_vertical"/>

    <TextView
            android:text="MARCA E MODELLO VEICOLO"
            android:layout_width="0dp"
            android:layout_height="26dp"
            android:id="@+id/textView33"
            app:layout_constraintStart_toStartOf="@+id/imageView11"
            android:layout_marginStart="8dp"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:background="@drawable/border_text1"
            android:textAlignment="center"
            android:fontFamily="@font/heebo_bold" android:typeface="normal" android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/textView32" android:gravity="center_horizontal|center_vertical"
            app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="25dp" app:layout_constraintTop_toTopOf="@+id/textView31"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="26dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextVeicolo"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView11" android:background="@drawable/border_text2"
            android:textAlignment="center" app:layout_constraintStart_toEndOf="@+id/textView31"
            android:fontFamily="@font/heebo" android:textSize="12sp" android:textColor="@color/colorPrimary"
            android:gravity="center_horizontal|center_vertical" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="25dp" app:layout_constraintTop_toTopOf="@+id/editText"/>

</android.support.constraint.ConstraintLayout>